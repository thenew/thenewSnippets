  0% {
    opacity: 0;
  }
  100% {
    opacity: 100%;
  }
}
@-moz-keyframes fade {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 100%;
  }
}

.selector {
  /* Animation name, duration, delay, iterations, easing */
  -webkit-animation: fade 1s 0 0 ease-out;
  -moz-animation: fade 1s 0 0 ease-out;
}]]>
